{
  "name": "javierleon9966/tracker",
  "description": "A PocketMine-MP plugin to track a player's location",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "require": {
    "pocketmine/pocketmine-mp": "^5.0.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.2.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "phpstan/extension-installer": "^1.0"
  },
  "autoload": {
    "psr-0": {
      "JavierLeon9966\\Tracker\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
